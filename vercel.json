{
    "builds": [
      {
        "src": "vercel_app/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.10" }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "vercel_app/wsgi.py"
      }
    ]
}
{
    "engines": {
      "node": "18.x"
    }
}